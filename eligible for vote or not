import java.util.Scanner;

class Launch {
	public static void main(String[] args) {
		int n;
		Scanner s = new Scanner(System.in);
		System.out.println("enter the age");
		n = s.nextInt();
		if (n >= 18)
			System.out.println("eligble for vote");
		else
			System.out.println("not eligble for vote");

	}
}
