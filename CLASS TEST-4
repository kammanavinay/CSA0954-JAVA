
Q1

package logo;

import java.io.*;
public class right 
{
	
	public static void StarRightTriangle(int n)
	{
		int a, b;

		
		for (a = 0; a < n; a++) {

			
			for (b = 0; b <= a; b++) {
				
				System.out.print("* ");
			}

			
			System.out.println();
		}
	}


	public static void main(String args[])
	{
		int k = 5;
		StarRightTriangle(k);
	}
}
Q2
package logo;

import java.util.*;
 class prime
{

public static void main(String[] args)
{
int numbers ;
Scanner s = new Scanner(System.in);
System.out.print("Enter a numbers:");
numbers = s.nextInt();
for(int i = 2; i <= numbers/2; i++)
{
if(numbers % i == 0)
System.out.println(numbers + " is a Prime number.");
else
System.out.println(numbers + " is not a Prime number\n(it is a Composite number.)");
}
}
}

Q3
package logo;

import java.util.Scanner;

class factor 
 {
 public static void main(String[] args)
 {
	    int n;
	    Scanner s=new Scanner(System.in);
 System.out.print("enter the factor ");
    n=s.nextInt();
	     for (int i = 1; i <= n; i++) {
	      if (n % i == 0) {
	        System.out.print(i + " ");
	      }
	    }
	  }
	}
