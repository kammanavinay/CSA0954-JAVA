package day5;

import java.util.*;
class pattern
{
	public static void main(String[]args)
	{
		int n,i,j,k;
		Scanner input=new Scanner(System.in);
		System.out.println("enter the rows");
	   n=input.nextInt();
		 k=1;
		for(i=1;i<=n;i++)
		{
		 for( j=1;j<=i;j++)
		 {
		 System.out.print(k*k+" ");
		 k++;
		 }
		 System.out.println();
		}
	}
}
