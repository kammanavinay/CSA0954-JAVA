package day5;

import java.util.*;
class factors
{
	public static void mian(String[]args)
	{
		int n,factors,i;
		Scanner input=new Scanner(System.in);
		System.out.println("enter the factor");
		 n=input.nextInt();
		factors=0;
		for( i=1;i<=n;i++)
		{
		 if(n%i==0)
		 factors=factors+1;
		}
		System.out.print("Number of factors = "+factors);
		 
	}
}
